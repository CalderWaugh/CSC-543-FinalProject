<%- include('partials/head') %>

<div class="content">
    <div style="padding-top: 50px;text-align: center;" class="container justify-content-center">
        <h3>Matching Patients</h3>
        <div style="padding: 0 25%;">
            <table class="table styler patient_results">
                <thead>
                    <th>Patient</th>
                </thead>
                <% patients.forEach(patient => { %>
                <tr>
                    <td><%= patient.first_name %> <%= patient.last_name %></td>
                    <td class="create-appointment-td"><a href="/patient/<%= patient.patient_id %>" class="btn btn-primary btn-sm">View Patient</a></td>
                </tr>
                <% }); %>
            </table>
        </div>
    </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>

