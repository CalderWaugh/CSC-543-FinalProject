<%- include('partials/head') %>

      <div class="content">
        <div style="padding-top: 50px;text-align: center;" class="row d-flex justify-content-center">
            <div class="card styler col-3">
                <div class="card-body">
                    <i class="bi bi-person-circle" style="font-size: 70px;"></i>
                    <h5 class="card-title"><%= user.user_type %>: <%= user.username %></h5>
                    <p class="card-text">Name: <strong><%= user.first_name %> <%= user.last_name %></strong></p>
                </div>
            </div>
        </div>
      </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </body>
</html>
